<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="10dp"
    android:layout_marginStart="2dp"
    android:layout_marginEnd="2dp"
    android:paddingBottom="2dp"
    android:backgroundTint="@color/white">

    <TextView
        android:id="@+id/tvTitleYoutubeChannelTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginStart="12dp"
        android:text="Profile : "
        android:textColor="@color/abuabu_tua"
        android:textSize="14sp"
        android:textStyle="normal"
        />

    <TextView
        android:id="@+id/tvYoutubeChannelTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/tvTitleYoutubeChannelTitle"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="12dp"
        android:layout_marginStart="6dp"
        android:text="Nama Profile"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="normal"
        />

    <TextView
        android:id="@+id/tvTitleYoutubeTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tvYoutubeChannelTitle"
        android:layout_marginTop="12dp"
        android:layout_marginStart="12dp"
        android:text="Judul : "
        android:textColor="@color/abuabu_tua"
        android:textSize="14sp"
        android:textStyle="normal"
        />

    <TextView
        android:id="@+id/tvYoutubeTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toEndOf="@id/tvTitleYoutubeTitle"
        app:layout_constraintTop_toBottomOf="@id/tvYoutubeChannelTitle"
        android:layout_marginTop="12dp"
        android:layout_marginStart="12dp"
        android:text="Nama judul"
        android:textColor="@color/black"
        android:textSize="14sp"
        android:textStyle="normal"
        />

</androidx.constraintlayout.widget.ConstraintLayout>
